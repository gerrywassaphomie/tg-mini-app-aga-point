{
  "version": 2,
  "framework": null,
  "builds": [
    { "src": "api/fetchStopice.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/fetchStopice", "dest": "/api/fetchStopice.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
